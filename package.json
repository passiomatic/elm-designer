{
  "name": "elm-designer",
  "productName": "Elm Designer",
  "version": "0.4.2",
  "description": "A code generator for Elm UI",
  "repository": "https://github.com/passiomatic/elm-designer",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "elm-constants": "^1.0.0",
    "elm-hot": "^1.1.6",
    "node-elm-compiler": "^5.0.6",
    "sass": "^1.43.4"
  },
  "dependencies": {
    "balloon-css": "^1.2.0",
    "parcel-bundler": "1.12.3"
  },
  "scripts": {
    "dev": "parcel src/app.html",
    "dev-no-debug": "NODE_ENV=production parcel src/app.html",
    "landing": "parcel src/index.html",
    "constants": "elm-constants",
    "build": "parcel build src/index.html --public-url ./ --out-file index.html",
    "build-no-maps": "parcel build src/index.html --no-source-maps --public-url ./ --out-file index.html",
    "clean": "rm -fR dist"
  }
}
